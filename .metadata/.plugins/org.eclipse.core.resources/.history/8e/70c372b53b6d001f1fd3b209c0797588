#include "stm32f4xx.h"
#define ADC1EN			(1U<<8)
#define GPIOAEN			(1U<<0)
#define
#define
#define


void pa1_adc_init(void)
{
	//*******Configure the ADC GPIO pin*******
	//Enable clock access to GPIOA
	RCC->AHB1ENR |= GPIOAEN;
	//Set the mode of PA1 to analog
	GPIOA->MODER |= (1U<<2);
	GPIOA->MODER |= (1U<<3);
	//*******Configure the ADC module*********
	//Enable clock access to ADC
	RCC->AHB2ENR |= ADC1EN;
	//Configure ADC parameters

}
